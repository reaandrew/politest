{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCrossAccountRead",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::111111111111:user/alice"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::shared-bucket/*"
    },
    {
      "Sid": "DenyCrossAccountWrite",
      "Effect": "Deny",
      "Principal": {
        "AWS": "arn:aws:iam::111111111111:user/alice"
      },
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::shared-bucket/*"
    }
  ]
}
