{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowAllExceptS3Write",
      "Effect": "Allow",
      "Action": "*",
      "Resource": "*"
    },
    {
      "Sid": "DenyS3WriteActions",
      "Effect": "Deny",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:PutObjectAcl"
      ],
      "Resource": "*"
    }
  ]
}
