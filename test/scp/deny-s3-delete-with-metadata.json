{
  "Version": "2012-10-17",
  "Description": "SCP with metadata - denies S3 delete operations",
  "OrganizationUnit": "Production",
  "AppliesTo": ["ou-1234", "ou-5678"],
  "Owner": "security-team",
  "Statement": [
    {
      "Sid": "DenyS3Delete",
      "Effect": "Deny",
      "Action": [
        "s3:DeleteObject",
        "s3:DeleteBucket"
      ],
      "Resource": "*",
      "Justification": "Prevent accidental deletion of S3 resources",
      "ExpiryDate": "2026-12-31"
    }
  ]
}
