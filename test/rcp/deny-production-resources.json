{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowAllExceptProduction",
      "Effect": "Allow",
      "Action": "*",
      "Resource": "*"
    },
    {
      "Sid": "DenyProductionResources",
      "Effect": "Deny",
      "Action": "*",
      "Resource": [
        "arn:aws:s3:::production-*",
        "arn:aws:s3:::production-*/*"
      ]
    }
  ]
}
